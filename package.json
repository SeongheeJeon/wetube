{
  "name": "git_wetube",
  "version": "1.0.0",
  "description": "Cloning Youtube with Vanilla JS and NodeJS",
  "main": "index.js",
  "author": "Seonghee Jeon",
  "license": "ISC",
  "engines": {
    "node": "14.17.0"
  },
  "dependencies": {
    "@babel/cli": "^7.13.16",
    "@babel/core": "^7.14.2",
    "@babel/node": "^7.14.2",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.13.9",
    "autoprefixer": "^10.2.5",
    "aws-sdk": "^2.895.0",
    "axios": "^0.21.1",
    "babel-core": "^7.0.0-bridge.0",
    "babel-loader": "^8.2.2",
    "body-parser": "^1.19.0",
    "connect-mongo": "^4.4.1",
    "cookie-parser": "^1.4.5",
    "css-loader": "^5.1.3",
    "dotenv": "^9.0.2",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "get-blob-duration": "^1.2.0",
    "helmet": "^4.4.1",
    "mini-css-extract-plugin": "^1.3.9",
    "mongoose": "^5.12.0",
    "morgan": "^1.10.0",
    "multer": "^1.4.2",
    "multer-s3": "^2.9.0",
    "node-sass": "^5.0.0",
    "passport": "^0.4.1",
    "passport-github": "^1.1.0",
    "passport-kakao": "^1.0.1",
    "passport-local": "^1.0.0",
    "passport-local-mongoose": "^6.1.0",
    "postcss-loader": "^5.2.0",
    "pug": "^3.0.2",
    "sass-loader": "^11.0.1",
    "webpack": "^5.26.3",
    "webpack-cli": "^4.5.0"
  },
  "scripts": {
    "dev:server": "nodemon --exec babel-node src/init.js --delay 2 --ignore 'static'",
    "dev:assets": "cd src && cross-env WEBPACK_ENV=development webpack -w",
    "build:assets": "cd src && cross-env WEBPACK_ENV=production webpack",
    "build:server": "babel src --out-dir build --ignore 'src/assets','src/static','src/webpack.config.js'",
    "copyAll": "shx cp -R src/static build && shx cp -R src/views build",
    "prebuild": "shx rm -rf build",
    "build": "npm run build:server && npm run build:assets && npm run copyAll",
    "start": "PRODUCTION=true node build/init.js"
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "eslint": "^7.22.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.3.1",
    "nodemon": "^2.0.7",
    "prettier": "^2.2.1",
    "shx": "^0.3.3"
  }
}
